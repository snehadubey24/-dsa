<h1> Given an array a[] of size n. For every i from 0 to n-1 output =max(a[0],a[1]...a[i]). </h1>
#include <iostream>
#include<bits/stdc++.h>


using namespace std;

int main() {
    
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++)
	{
	    cin>>a[i];
	}
	int mx=a[0];
	for(int i=0;i<n;i++)
	{
	    mx=max(mx,a[i]);
	    cout<<mx<<" ";
	}

	 
	return 0;
}
